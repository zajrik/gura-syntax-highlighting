{
	"name": "gura-syntax-highlight",
	"displayName": "Gura Syntax Highlighting",
	"description": "Provides syntax highlighting for the Gura configuration format",
	"version": "1.0.0",
	"repository": {
		"url": "https://github.com/zajrik/gura-syntax-highlight"
	},
	"publisher": "zajrik",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Programming Languages"
	],
	"icon": "fileicons/images/gura.png",
	"contributes": {
		"languages": [{
			"id": "gura",
			"aliases": ["Gura"],
			"extensions": ["ura"],
			"configuration": "./language-configuration.json"
		}],
		"grammars": [{
			"language": "gura",
			"scopeName": "source.ura",
			"path": "./syntaxes/gura.tmLanguage.json"
		}]
	}
}
